{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "pretty": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "types": ["miniprogram-api-typings"],
    "lib": ["esnext", "dom"]
  },
  "include": ["src", "node_modules/@jswork"]
}
